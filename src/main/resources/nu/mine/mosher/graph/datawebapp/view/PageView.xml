<?xml version="1.0" encoding="UTF-8"?>
<wicket:extend xmlns="http://www.w3.org/1999/xhtml" xmlns:wicket="http://wicket.apache.org">
    <button wicket:id="list"><wicket:message key="list"/></button>
    <h3 wicket:id="entity"/>
    <button wicket:id="edit"><wicket:message key="edit"/></button>
    <table>
        <tbody>
            <tr wicket:id="properties">
                <td><span wicket:id="name"/></td>
                <td><b><span wicket:id="property"/></b></td>
            </tr>
        </tbody>
    </table>
    <ul>
        <li wicket:id="refsMultiple">
            <wicket:container wicket:id="name"/>:
            <ul style="margin-left:0; padding-left:40px;">
                <li wicket:id="ref">
                    <a wicket:id="link"><b><span wicket:id="entity"/></b></a>
                    <button wicket:id="remove"><wicket:message key="remove"/></button>
                </li>
                <li wicket:id="empty">
                    <wicket:message key="empty"/>
                </li>
                <li>
                    <button wicket:id="add"><wicket:message key="add"/></button>
                </li>
            </ul>
        </li>
        <li wicket:id="refsSingular">
            <wicket:container wicket:id="name"/>:
            <a wicket:id="link"><b><span wicket:id="entity"/></b></a>
            <button wicket:id="remove"><wicket:message key="remove"/></button>
            <span wicket:id="empty"><wicket:message key="empty"/></span>
            <button wicket:id="add"><wicket:message key="add"/></button>
        </li>
    </ul>
    <p>
        <button wicket:id="delete"><wicket:message key="delete"/></button>
    </p>
</wicket:extend>
