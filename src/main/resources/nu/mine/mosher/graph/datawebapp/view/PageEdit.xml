<?xml version="1.0" encoding="UTF-8"?>
<wicket:extend xmlns="http://www.w3.org/1999/xhtml" xmlns:wicket="http://wicket.apache.org">
    <h3 wicket:id="entity"/>
    <form wicket:id="form">
        <ul>
            <li wicket:id="properties">
                <label wicket:id="label">
                    <wicket:container wicket:id="name"/>:
                    <input wicket:id="property"/>
                </label>
            </li>
        </ul>
        <p>
            <button wicket:id="cancel"><wicket:message key="cancel"/></button>
            <input type="submit" value="save"/>
        </p>
    </form>
</wicket:extend>
