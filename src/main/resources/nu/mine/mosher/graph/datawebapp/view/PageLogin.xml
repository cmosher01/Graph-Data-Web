<?xml version="1.0" encoding="UTF-8"?>
<wicket:extend xmlns="http://www.w3.org/1999/xhtml" xmlns:wicket="http://wicket.apache.org">
    <form wicket:id="form">
        <table style="width:100%">
            <colgroup>
                <col style="width:fit-content"/>
                <col style="width:auto"/>
            </colgroup>
            <tbody>
                <tr>
                    <td>username:</td>
                    <td><input style="font-family:unset; font-size:unset" wicket:id="username"/></td>
                </tr>
                <tr>
                    <td>password:</td>
                    <td><input style="font-family:unset; font-size:unset" type="password" wicket:id="password"/></td>
                </tr>
            </tbody>
        </table>
        <input type="submit" value="Log in" />
    </form>
</wicket:extend>
